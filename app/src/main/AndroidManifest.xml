<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.solarix.bd.eshitafor">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_love"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_love"
        android:supportsRtl="true"
        android:theme="@style/Theme.EshitaFor"
        android:usesCleartextTraffic="true">
        <activity android:name=".Lastpage"
            android:theme="@style/Theme.AppCompat.NoActionBar"></activity>
        <activity android:name=".FinalPage" />
        <activity android:name=".QuestionsPage" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

</manifest>